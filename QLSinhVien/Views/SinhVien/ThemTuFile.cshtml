
@{
    ViewBag.Title = "ThemTuFile";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Thêm Sinh Viên Từ File Excel</h2>

@using (Html.BeginForm("ThemTuFile", "SinhVien", null, FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary()
    <div class="form-group">
        <input type="file" id="dataFile" name="upload" class="btn btn-info" />
    </div>
        <div class="form-group">
            @Html.ActionLink("Hủy", "Index", "SinhVien", null, new { @class = "btn btn-default" })
            <input type="submit" value="Tải lên và Lưu" class="btn btn-success" />
        </div>
}
